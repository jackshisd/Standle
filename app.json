{
  "expo": {
    "name": "Standle",
    "slug": "standle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "entryPoint": "./App.js",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "jsEngine": "jsc"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jmshi.standle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jmshi.standle"
    },
    "web": {
      "favicon": "./assets/standlefavicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "sdkVersion": "50.0.0",
    "plugins": ["expo-font", "expo-router"],
    "extra": {
      "eas": {
        "projectId": "f4ab06e8-7cb6-4a7d-ac58-385c23f834b1"
      }
    },
    "owner": "jmshi"
  },
  "name": "standle",
  "scheme": "your-app-scheme"
}
